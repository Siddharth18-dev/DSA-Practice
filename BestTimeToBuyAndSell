class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int BuyDay = INT_MAX;
        int  maxProfit = 0;
        int n = prices.size();
        for(int i = 0; i < n; i++){
            BuyDay = min(BuyDay, prices[i]);
            maxProfit = max(maxProfit, prices[i] - BuyDay);
        }
        return maxProfit;
    }
};
